<!-- app/views/active_sessions/_active_session.html.erb -->
<tr>
  <td class="text-gray-500 dark:text-gray-400"><%= active_session.user_agent %></td>
  <td class="text-gray-500 dark:text-gray-400"><%= active_session.ip_address %></td>
  <td class="text-gray-500 dark:text-gray-400"><%= active_session.created_at %></td>
  <td class="text-center">
    <%= button_to active_session_path(active_session), method: :delete do %>
      <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6 text-red-600 dark:text-red-500" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
        <path stroke-linecap="round" stroke-linejoin="round" d="M10 14l2-2m0 0l2-2m-2 2l-2-2m2 2l2 2m7-2a9 9 0 11-18 0 9 9 0 0118 0z" />
      </svg>
    <% end %>
  </td>
</tr>
